<h1><%= t :create_crm_google_contact_sync %></h1>

<% form_for(@crm_google_contact_sync) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label t(:email) %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label t(:lead_group) %><br />
    <%= f.text_field :lead_group %>
  </p>
  <p>
    <%= f.label t(:contact_group) %><br />
    <%= f.text_field :contact_group %>
  </p>
  <p>
    <%= f.submit t(:create) %>
  </p>
<% end %>
<%= link_to t(:back), url_for(:back) %>